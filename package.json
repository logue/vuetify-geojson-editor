{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vuetify-geojson-editor",
  "description": "Vuetify GeoJson Editor",
  "version": "0.0.9",
  "license": "MIT",
  "type": "module",
  "private": true,
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/vuetify-geojson-editor",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/vuetify-geojson-editor.git"
  },
  "bugs": {
    "url": "https://github.com/logue/vuetify-geojson-editor/issues"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0",
    "pnpm": ">=10.2.0"
  },
  "packageManager": "pnpm@10.23.0",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf ./node_modules/.vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint && prettier \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "prepare": "husky"
  },
  "dependencies": {
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-markdown": "^6.5.0",
    "@mdi/font": "^7.4.47",
    "@turf/clean-coords": "^7.3.0",
    "@turf/rewind": "^7.3.0",
    "axios": "^1.13.2",
    "chroma-js": "^3.1.2",
    "core-js": "^3.47.0",
    "es-toolkit": "^1.42.0",
    "geojson": "^0.5.0",
    "geojson-precision-ts": "^1.4.0",
    "json-url": "^4.0.0",
    "ol": "^10.7.0",
    "ol-ext": "^4.0.36",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "streamsaver": "^2.0.6",
    "topojson-client": "^3.1.0",
    "topojson-server": "^3.0.1",
    "uuid": "^13.0.0",
    "vue": "^3.5.25",
    "vue-codemirror6": "^1.4.1",
    "vue-router": "^4.6.3",
    "vuetify": "^3.10.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tsconfig/node-lts": "^24.0.0",
    "@types/chroma-js": "^3.1.2",
    "@types/geojson": "^7946.0.16",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@types/ol-ext": "npm:@siedlerchr/types-ol-ext@^3.6.5",
    "@types/streamsaver": "^2.0.5",
    "@types/topojson-client": "^3.1.5",
    "@types/topojson-server": "^3.0.4",
    "@types/topojson-specification": "^1.0.5",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/eslint-plugin": "1.4.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-playwright": "^2.3.0",
    "eslint-plugin-vue": "^10.6.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-plugin-vuetify": "^2.5.3",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "npm-run-all2": "^8.0.4",
    "postcss-html": "^1.8.0",
    "prettier": "^3.6.2",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^6.1.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass-embedded": "^1.93.3",
    "stylelint": "^16.26.0",
    "stylelint-config-recommended-scss": "^16.0.2",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-order": "^7.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.4",
    "vite-plugin-checker": "0.11.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^4.0.13",
    "vue-tsc": "^3.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yaml,yml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "json5": ">=2.2.3",
    "yaml": ">=2.3.2",
    "core-js": ">=3.42.0"
  },
  "stackblitz": {
    "startCommand": "pnpm run test:unit"
  }
}